# Copy file này thành .env và điền giá trị thật

GEMINI_API_KEY=your_gemini_api_key

# PostgreSQL
# Cách 1: Connection string (Neon, Supabase, Vercel Postgres)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DATABASE?sslmode=require

# Cách 2: Biến rời
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=camguard_pro
# DB_USER=postgres
# DB_PASSWORD=your_password
# DB_SSL=false   # Đặt false nếu DB không hỗ trợ SSL
